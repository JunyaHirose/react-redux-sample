{
  "name": "react-typescript-redux",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.dev.js",
    "watch": "webpack --config webpack.config.dev.js --watch",
    "build:prod": "webpack --config webpack.config.prod.js",
    "test:unit": "karma start karma.conf.js",
    "test:all": "karma start karma.conf.js **/*.spec.tsx **/*.spec.ts",
    "server": "node dev-server.js",
    "lint:unit": "tslint -c tslint.json",
    "lint:all": "tslint -c tslint.json 'src/**/*.ts?(x)'",
    "format-base": "prettier --config ./prettier.config.js",
    "format-check:all": "npm run format-base -- --list-different 'src/**/*.ts?(x)'",
    "format-check:unit": "npm run format-base -- --list-different",
    "format:all": "npm run format-base -- --write ./prettier.config.js 'src/**/*.ts?(x)'",
    "format:unit": "npm run format-base -- --write ./prettier.config.js"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.9",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/fetch-mock": "6.0.1",
    "@types/jasmine": "2.8.6",
    "@types/react": "16.1.0",
    "@types/react-dom": "16.0.4",
    "@types/react-redux": "5.0.15",
    "@types/react-router-dom": "4.2.6",
    "core-js": "2.5.4",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "express": "4.16.3",
    "fetch-mock": "6.3.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-webpack": "3.0.0",
    "prettier": "1.11.1",
    "react-test-renderer": "16.3.0",
    "ts-loader": "4.1.0",
    "tslint": "5.9.1",
    "tslint-config-standard": "7.0.0",
    "tslint-react": "3.5.1",
    "typescript": "2.8.1",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13"
  },
  "dependencies": {
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2"
  }
}
